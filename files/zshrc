# zmodload zsh/zprof

if [[ $- != *i* ]] ; then
    # Shell is non-interactive.  Be done now!
    return
fi

# get the require function
PACKAGES=${HOME}/.shutils
source ${PACKAGES}/init.sh &> /dev/null || return

require-if-exists base

require-if-exists localpre

require aliases
require autoenv
require completion
require custom-cd
require dircolors
require prompt
require python-utils
require node-utils
require ssh-agent

require base-vars

require-if-exists plugins
require-if-exists keyboard
require-if-exists localpost

cd .

# zprof
